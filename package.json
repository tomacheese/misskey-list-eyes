{
  "name": "misskey-list-eyes",
  "version": "1.0.0",
  "main": "output/index.js",
  "repository": "https://github.com/tomacheese/misskey-list-eyes",
  "author": "Tomachi",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@book000/node-utils": "1.6.24",
    "@types/node": "18.18.5",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "@vercel/ncc": "0.38.0",
    "axios": "1.5.1",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.2.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "form-data": "4.0.0",
    "prettier": "3.0.3",
    "puppeteer-core": "21.3.8",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2",
    "yarn-run-all": "3.1.1"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev --poll -r tsconfig-paths/register ./src/main.ts",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/",
    "compile": "tsc -p .",
    "clean": "rimraf dist output",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  }
}
